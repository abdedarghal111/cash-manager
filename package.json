  "name": "cash_manager_server",
  "version": "0.7.3",
  "license": "MIT",
  "main": "dist/electron/main/electronMain.js",
  "scripts": {
    "server:build": "node buildFiles/backend.mjs --dev",
    "server:build:watch": "node buildFiles/backend.mjs --dev --watch",
    "server:run:watch": "nodemon --watch dist/backend/ dist/backend/main.server.mjs --dev",
    "server:dev": "pnpm run server:build && concurrently \"pnpm run server:build:watch\" \"pnpm run server:run:watch\"",
    "server:run": "node dist/backend/main.server.mjs",
    "client:dev": "vite --mode development --config buildFiles/frontend.dev.mjs",
    "client:build": "vite build --config buildFiles/frontend.prod.mjs",
    "client:build:watch": "vite build --config buildFiles/frontend.prod.mjs --watch",
    "client:check": "svelte-check --tsconfig tsconfig.json && tsc -p tsconfig.json",
    "dev": "concurrently \"pnpm run server:dev\" \"pnpm run client:dev\"",
    "tests:run": "pnpm run server:build && node --enable-source-maps buildFiles/jasmineConfig.mjs",
    "deploy:build": "pnpm run server:build && pnpm run client:build",
    "deploy:start": "pm2 start dist/app.mjs --name cashms --log serverData/app.log",
    "deploy:stop": "pm2 stop cashms",
    "electron:preview": "electron-vite preview --config buildFiles/electron/electron.vite.config.mjs --entry dist/electron/main/electronMain.js",
    "electron:dev": "electron-vite --mode development --config buildFiles/electron/electron.vite.config.mjs --entry dist/electron/main/electronMain.js",
    "electron:build": "pnpm run client:check && electron-vite build --config buildFiles/electron/electron.vite.config.mjs --entry dist/electron/main/electronMain.js",
    "electron:build:unpack": "pnpm run electron:build && electron-builder --dir",
    "build:win": "pnpm run electron:build && electron-builder --win",
    "build:mac": "pnpm run electron:build && electron-builder --mac",
    "build:linux": "pnpm run electron:build && electron-builder --linux",
    "build:android": "pnpm run client:build && pnpm run cap:prepareAndroid && cd android && gradlew assembleDebug && cd ..",
    "electron:release": "electron-builder",
    "cap:prepareAndroid": "cap add android && cap sync && capacitor-assets generate --assetPath assets --android",
    "terminal": "pnpm run server:build && node dist/backend/scripts/main.server.mjs"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/core": "^8.0.2",
    "@capacitor/splash-screen": "^8.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/bcrypt": "^5.0.2",
    "@types/ws": "^8.18.1",
    "@zag-js/svelte": "^1.33.1",
    "axios": "^1.13.4",
    "bcrypt": "^5.1.1",
    "colors": "^1.4.0",
    "concurrently": "^9.2.1",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parse": "^0.4.0",
    "cross-env": "^7.0.3",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.6.1",
    "electron-updater": "^6.7.3",
    "esbuild": "^0.25.12",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "esbuild-svelte": "^0.9.4",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "helmet": "^8.1.0",
    "lucide": "^0.503.0",
    "lucide-svelte": "^0.503.0",
    "node-forge": "^1.3.3",
    "nodemon": "^3.1.11",
    "pm2": "^6.0.14",
    "postcss": "^8.5.6",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "svelte-fa": "^4.0.4",
    "svelte-french-toast": "^1.2.0",
    "svelte-modals": "^2.0.1",
    "tailwindcss": "^4.1.18",
    "tsconfig": "^7.0.0",
    "vite": "^6.4.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "latest",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jasmine": "^5.1.15",
    "@types/node": "^22.19.7",
    "@types/node-forge": "^1.3.14",
    "@types/ws": "^8.5.5",
    "electron": "^38.8.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "jasmine": "^5.13.0",
    "pnpm": "^10.28.2",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.6",
    "typescript": "~5.7.3",
    "vite": "^7.1.6"
  }
}
